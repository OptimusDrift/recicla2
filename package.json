{
  "name": "Recicla2",
  "version": "1.0.0",
  "description": "Un juego realizado para la Unraf",
  "scripts": {
    "clear-build-cache": "rm -rf .cache/ dist/",
    "dev": "parcel src/index.html",
    "start": "parcel src/index.html -p 8000",
    "build": "cross-env NODE_ENV=production parcel build ./src/index.html --public-url ./",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "OptimusDrift",
  "license": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/OptimusDrift/recicla2"
  },
  "homepage": "https://github.com/OptimusDrift/recicla2",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^8.1.0",
    "minimist": ">=1.2.2",
    "parcel-bundler": "^1.9.4",
    "parcel-plugin-clean-easy": "^1.0.2",
    "parcel-plugin-static-files-copy": "^2.4.3",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@firebase/app": "^0.7.6",
    "@firebase/app-types": "^0.7.0",
    "@firebase/database": "^0.12.3",
    "@firebase/database-types": "^0.9.2",
    "array-shuffle": "^3.0.0",
    "asset": "^0.4.13",
    "csv": "^5.5.3",
    "firebase": "^9.3.0",
    "firebase-admin": "^10.0.0",
    "firebase-firestore-lite": "^1.0.3",
    "firebase-tools": "^7.1.1",
    "mongodb": "^4.1.4",
    "mongoose": "^6.0.12",
    "phaser": "^3.55.2",
    "public": "^0.1.4",
    "webfontloader": "^1.6.28"
  },
  "parcelCleanPaths": [
    "dist"
  ],
  "staticFiles": {
    "staticPath": "public",
    "watcherGlob": "**"
  }
}
